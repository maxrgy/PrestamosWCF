<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="PrestamosServicios"
                   namespace="PrestamosServicios.Dominio"
                   default-lazy="false">
  <class name="Equipo" table="Equipo">
    <id name="Codigo" column="co_equipo">
      <generator class="increment" />
    </id>
    <property name="Modelo" column="co_modelo" />
    <property name="Estado" column="estado" />
    <property name="Categoria" column="co_categoria" />
  </class>
</hibernate-mapping>