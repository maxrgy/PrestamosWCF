<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="PrestamosServicios"
                   namespace="PrestamosServicios.Dominio"
                   default-lazy="false">
