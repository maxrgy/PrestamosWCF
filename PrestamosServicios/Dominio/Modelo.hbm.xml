<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="PrestamosServicios"
                   namespace="PrestamosServicios.Dominio"
                   default-lazy="false">
  <class name="Modelo" table="Modelo">
    <id name="Codigo" column="co_modelo">
      <generator class="increment" />
    </id>
    <property name="Model" column="modelo" />
    <many-to-one name="CoCategoria" column="co_categoria" />
  </class>
</hibernate-mapping>
